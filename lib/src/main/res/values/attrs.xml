<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NumberCircleProgressBar">
        <attr name="ncpb_progress" format="integer" />
        <attr name="ncpb_max" format="integer" />
        <attr name="ncpb_progress_unreached_color" format="color" />
        <attr name="ncpb_progress_reached_color" format="color" />
        <attr name="ncpb_progress_text_size" format="dimension" />
        <attr name="ncpb_progress_text_color" format="color" />
        <attr name="ncpb_progress_circle_radius" format="dimension" />
        <attr name="ncpb_progress_stroke_width" format="dimension" />
        <attr name="ncpb_progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
        <attr name="ncpb_progress_fill_mode" format="enum">
            <enum name="rotate" value="0" />
            <enum name="rising_water" value="1" />
            <enum name="ring" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="Themes">
        <attr name="numberCircleProgressBarStyle" format="reference" />
    </declare-styleable>

</resources>